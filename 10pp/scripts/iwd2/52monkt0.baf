IF
  !TimerActive(4)
  NearSavedLocation(1)
THEN
  RESPONSE #100
    StartRandomTimer(4,1,3)
    SetGlobal("FLOAT_NOISE","LOCALS",1)
    PlaySequence(Myself,ATTACK)
    Continue()
  RESPONSE #100
    StartRandomTimer(4,1,3)
    SetGlobal("FLOAT_NOISE","LOCALS",1)
    PlaySequence(Myself,ATTACK_BACKSLASH)
    Continue()
  RESPONSE #100
    StartRandomTimer(4,1,3)
    SetGlobal("FLOAT_NOISE","LOCALS",1)
    PlaySequence(Myself,ATTACK_JAB)
    Continue()
END

IF
  !Global("FLOAT_NOISE","LOCALS",0)
THEN
  RESPONSE #33
