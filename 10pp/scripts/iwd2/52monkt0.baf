    SetGlobal("FLOAT_NOISE","LOCALS",0)
  RESPONSE #33
    FloatMessage(Myself,20774)  // ~"*HAI!*"~
    SetGlobal("FLOAT_NOISE","LOCALS",0)
  RESPONSE #33
    FloatMessage(Myself,20775)  // ~"*Hanh!*"~
    SetGlobal("FLOAT_NOISE","LOCALS",0)
  RESPONSE #201
