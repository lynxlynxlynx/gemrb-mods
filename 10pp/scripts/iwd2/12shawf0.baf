IF
  CreatureHidden(Myself)
  GlobalGT("Goblin_Palisade_Quest","GLOBAL",0)
THEN
  RESPONSE #100
    HideCreature(Myself,FALSE)
    Continue()
END

IF
  !CreatureHidden(Myself)
  GlobalGT("Goblin_Palisade_Quest","GLOBAL",0)
THEN
  RESPONSE #100
    ChangeCurrentScript("")
END

IF
  CreatureHidden(Myself)
THEN
  RESPONSE #100
    NoAction()
END

