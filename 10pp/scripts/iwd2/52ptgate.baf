IF
  Global("52Ven_Loot","GLOBAL",2)
  Opened([PC])
THEN
  RESPONSE #100
    StartCutSceneMode()
    ClearAllActions()
    MultiPlayerSync()
    ClearActions(Player1)
    ClearActions(Player2)
    ClearActions(Player3)
    ClearActions(Player4)
    ClearActions(Player5)
    ClearActions(Player6)
    StartCutScene("52cVen0")
END

IF
  !Global("52Ven_Loot","GLOBAL",2)
  Opened([PC])
THEN
  RESPONSE #100
    NoAction()
END

