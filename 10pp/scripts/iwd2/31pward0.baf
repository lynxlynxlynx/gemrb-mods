IF
  CheckDoorFlags(Myself,DOORLOCKED)
  PartyHasItem("30BStone")  // ~Black Ward Stone~
  PartyHasItem("30IStone")  // ~Iron Ward Stone~
THEN
  RESPONSE #100
    Unlock(Myself)
END

IF
  !CheckDoorFlags(Myself,DOORLOCKED)
  Or(2)
    !PartyHasItem("30BStone")  // ~Black Ward Stone~
    !PartyHasItem("30IStone")  // ~Iron Ward Stone~
THEN
  RESPONSE #100
    Lock(Myself)
END

