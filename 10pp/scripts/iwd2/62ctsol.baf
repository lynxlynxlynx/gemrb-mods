IF
  True()
THEN
  RESPONSE #100
    CutSceneId("Tsol_Silverblade")  // 
    MultiPlayerSync()
    FadeToColor([0.0],0)
    Wait(1)
    HideCreature(Myself,TRUE)
    Wait(1)
    FadeFromColor([0.0],0)
    Wait(1)
    EndCutSceneMode()
    DestroySelf()
END

