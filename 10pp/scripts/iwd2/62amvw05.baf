IF
  Global("RW_62AMVW050","LOCALS",0)
  !NearSavedLocation(5)
THEN
  RESPONSE #100
    SetGlobal("RW_62AMVW050","LOCALS",1)
    Continue()
END

IF
  !Global("RW_62AMVW050","LOCALS",0)
  NearSavedLocation(5)
THEN
  RESPONSE #100
    SetGlobal("RW_62AMVW050","LOCALS",0)
    NoAction()
END

IF
  !Global("RW_62AMVW050","LOCALS",0)
THEN
  RESPONSE #100
    MoveToPoint([-2.-2])
END

IF
  Global("RW_62AMVW050","LOCALS",0)
THEN
  RESPONSE #100
    RandomWalk()
END

