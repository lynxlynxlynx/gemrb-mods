IF
  !Global("10SwiftH_Dead","GLOBAL",0)
  !Dead(Myself)  // 
THEN
  RESPONSE #100
    DestroySelf()
END

IF
  !CreatureHidden(Myself)
  Global("Jump_Check","GLOBAL",0)
THEN
  RESPONSE #100
    HideCreature(Myself,TRUE)
    Continue()
END

IF
  CreatureHidden(Myself)
  GlobalGT("Jump_Check","GLOBAL",0)
THEN
  RESPONSE #100
    HideCreature(Myself,FALSE)
    Continue()
END

IF
  CreatureHidden(Myself)
THEN
  RESPONSE #100
    NoAction()
END

