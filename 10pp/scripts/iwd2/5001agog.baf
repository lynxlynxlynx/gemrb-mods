IF
  !Global("LIHMA_OUTSIDE","GLOBAL",0)
  Global("50Agog_Dead","GLOBAL",0)
THEN
  RESPONSE #100
    Enemy()
    Wait(2)
    FaceObject(NearestEnemyOf(Myself))
    SetGlobal("AGOG_OUTSIDE","GLOBAL",1)
    HideCreature(Myself,FALSE)
    ChangeCurrentScript("")
END

IF
  CreatureHidden(Myself)
THEN
  RESPONSE #100
    NoAction()
END

