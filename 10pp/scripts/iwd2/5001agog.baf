IF
  CreatureHidden(Myself)
  !Global("50Agog_Dead","GLOBAL",0)
THEN
  RESPONSE #100
    DestroySelf()
END

IF
  Global("5005_DRAGON_HP","GLOBAL",100)
  CreatureHidden(Myself)
  !HPPercent(Myself,100)
THEN
  RESPONSE #100
    SetHPPercent(Myself,100,MAX_HP)
    Continue()
END

