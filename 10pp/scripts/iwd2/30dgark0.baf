IF
  Global("GARK_TALK","MYAREA",2)
THEN
  RESPONSE #100
    SetGlobal("GARK_TALK","MYAREA",0)
END

IF
  OpenState(Myself,TRUE)
THEN
  RESPONSE #100
    ChangeCurrentScript("")
END

IF
  Global("GARK_TALK","MYAREA",0)
  !PartyHasItem("30keywar")  // ~Warren Key~
  !Dead("17_OgrGar")  // 
  Clicked([PC])
THEN
  RESPONSE #100
    SetGlobal("GARK_TALK","MYAREA",1)
END

