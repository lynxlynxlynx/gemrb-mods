IF
  Allegiance(Myself,ENEMY)
THEN
  RESPONSE #100
    ChangeCurrentScript("")
END

IF
  AttackedBy([GOODCUTOFF],0)
THEN
  RESPONSE #100
    Enemy()
END

IF
  IsScriptName("Du_Fighter_Thrall16",Myself)
THEN
  RESPONSE #100
    Face(S)
    Continue()
END

IF
  IsScriptName("Du_Fighter_Thrall17",Myself)
THEN
  RESPONSE #100
    Face(NE)
    Continue()
END

IF
  True()
THEN
  RESPONSE #100
    PlaySequence(Myself,ATTACK)
    WaitRandom(3,5)
    Continue()
END

