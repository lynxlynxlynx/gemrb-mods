IF
  CreatureHidden(Myself)
  !Global("Goblins_Attack_Palisade","GLOBAL",0)
THEN
  RESPONSE #100
    HideCreature(Myself,FALSE)
    Continue()
END

IF
  !Global("Goblins_Attack_Palisade","GLOBAL",0)
  !CreatureHidden(Myself)
THEN
  RESPONSE #100
    ChangeCurrentScript("")
END

IF
  CreatureHidden(Myself)
THEN
  RESPONSE #100
    NoAction()
END

