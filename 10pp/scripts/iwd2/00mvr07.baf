IF
  Global("RW_00MVR070","LOCALS",0)
  !NearSavedLocation(7)
THEN
  RESPONSE #100
    SetGlobal("RW_00MVR070","LOCALS",1)
    Continue()
END

IF
  !Global("RW_00MVR070","LOCALS",0)
  NearSavedLocation(7)
THEN
  RESPONSE #100
    SetGlobal("RW_00MVR070","LOCALS",0)
    NoAction()
END

IF
  !Global("RW_00MVR070","LOCALS",0)
THEN
  RESPONSE #100
    MoveToPoint([-2.-2])
END

IF
  !TimerActive(19)
  Global("RW_00MVR070","LOCALS",0)
THEN
  RESPONSE #100
    RandomWalk()
    StartRandomTimer(19,2,5)
END

