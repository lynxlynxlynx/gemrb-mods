    Wait(3)
    MoveViewObject([PC][0.0.10000.10000],20)
    Wait(2)
    UnhideGUI()
    MultiPlayerSync()
    SetVisualRange(0)
    MultiPlayerSync()
    EndCutSceneMode()
END

