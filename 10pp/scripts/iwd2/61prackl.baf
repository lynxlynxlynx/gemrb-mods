IF
  Clicked(Player1)
  Global("RackVictim","MYAREA",1)
THEN
  RESPONSE #100
    PlaySequence(Player1,AWAKE)
    ActionOverride(Player1,JumpToPoint([2944.1536]))
    DisplayString(Player1,34925)  // ~You climb down from the rack.~
    SetGlobal("RackVictim","MYAREA",0)
    TriggerActivation(Myself,FALSE)
END

