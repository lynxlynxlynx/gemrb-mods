IF
  Global("61CheckFloorCollapse","GLOBAL",0)
  !BitGlobal("61FloorCollapsed","GLOBAL",1,AND)
  IsOverMe(Player1)
THEN
  RESPONSE #100
    ClearSpriteEffects(Player1)
    IncrementGlobal("61FloorCollapsed","GLOBAL",1)
    Continue()
END

