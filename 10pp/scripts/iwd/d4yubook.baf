IF
  Clicked([PC])
  Global("YUANTI_BOOKS","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobal("YUANTI_BOOKS","GLOBAL",1)
    SetGlobal("YUANTI_SURPRISE","GLOBAL",1)
END

IF
  Unlocked([PC])
  Global("YUANTI_BOOKS","GLOBAL",0)
THEN
  RESPONSE #100
    SetGlobal("YUANTI_BOOKS","GLOBAL",1)
    SetGlobal("YUANTI_SURPRISE","GLOBAL",1)
END

