IF
  !Global("YUANTI_INFERNO","GLOBAL",0)
THEN
  RESPONSE #100
    OpenDoor(Myself)
    ChangeCurrentScript("")
END

IF
  Opened([PC])
  GlobalLT("GEELO_TRICK","GLOBAL",2)
THEN
  RESPONSE #100
    SetGlobal("YUANTI_INFERNO","GLOBAL",1)
END

IF
  Unlocked([PC])
  GlobalLT("GEELO_TRICK","GLOBAL",2)
THEN
  RESPONSE #100
    SetGlobal("YUANTI_INFERNO","GLOBAL",1)
END

