IF
  IsOverMe("Imoen")  // ~Imoen~
  CombatCounter(0)
  See(Player1)
  Global("ImoenLibrary","AR0602",0)
  OR(2)
    Global("ImoenClones","AR0602",0)
    Global("ImoenClones","AR0602",2)
THEN
  RESPONSE #100
    SetGlobal("ImoenLibrary","AR0602",1)
    ActionOverride("Imoen",StartDialogNoSet(Player1))
END

IF
  IsOverMe("Imoen")  // ~Imoen~
  CombatCounter(0)
  See(Player1)
  Global("ImoenLibrary","AR0602",0)
  Global("ImoenClones","AR0602",1)
THEN
  RESPONSE #100
    SetGlobal("ImoenClones","AR0602",2)
    SetGlobal("ImoenLibrary","AR0602",1)
    ActionOverride("Imoen",StartDialogNoSet(Player1))
END

