IF
  HPLT(Myself,20)
THEN
  RESPONSE #100
    StartDialogNoSet(Player1)
END

IF
  Die()
THEN
  RESPONSE #100
    ReallyForceSpellDead(Myself,VAMPIRE_GAS_FORM_CHANGE)
END

IF
  Allegiance(Myself,ENEMY)
  See([PC])
  Delay(12)
THEN
  RESPONSE #100
    ReallyForceSpellDead([PC],BEHOLDER_DOMINATION)
END

